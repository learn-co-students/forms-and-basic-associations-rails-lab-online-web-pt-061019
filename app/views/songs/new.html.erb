<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
 
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
</br>
<%= f.fields_for :notes  do |n|%>
<%= n.text_field :content %>
<%end%>
  <%= f.submit %>
<% end %>


